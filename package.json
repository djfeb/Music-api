{
  "name": "music-api",
  "version": "1.0.0",
  "description": "Simple Music API for accessing music database",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node run-migration-safe.js",
    "fix-indexes": "node fix-indexes.js",
    "download-images": "node download-images.js",
    "sync-images": "node sync-new-images.js",
    "check-images": "node check-images-status.js",
    "cleanup-images": "node cleanup-orphaned-images.js",
    "setup-images": "node run-migration-safe.js && node download-images.js"
  },
  "dependencies": {
    "@codyduong/ytmusicapi": "^0.4.6",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "mysql2": "^3.14.3",
    "puppeteer": "^24.17.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sanitize-filename": "^1.6.3",
    "sqlite3": "^5.1.7",
    "youtube-dl-exec": "^3.0.24"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  },
  "keywords": [
    "music",
    "api",
    "database"
  ],
  "author": "",
  "license": "MIT"
}
